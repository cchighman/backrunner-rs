	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "attach",
			"program": "${workspaceRoot}/target/debug/backrunner-rs.exe",
			"name": "Attach",
            "cwd": "${workspaceRoot}/target/debug/",
            "sourceLanguages": ["rust"]
		},

        {
            "type": "lldb",
            "request": "launch",
            "name": "Rust Debug Launch",
            "program": "${workspaceRoot}/target/debug/backrunner-rs.exe",
            "args": [],
            "cwd": "c:/Users/chhighma/source/repos/mev-determined/",
            "sourceLanguages": ["rust"]
        },
		

		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in library 'backrunner-rs'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--lib",
					"--package=backrunner-rs"
				],
				"filter": {
					"name": "backrunner-rs",
					"kind": "lib"
				}
			},
			"args": [],
